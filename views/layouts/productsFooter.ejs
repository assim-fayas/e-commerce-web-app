<script>
    function deletewishlist(proid) {

        Swal.fire({
            title: 'Are you sure?',
            text: "You wan't to delete!",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'yes, delete!'
        }).then((result) => {
            if (result.isConfirmed) {
                $.ajax({
                    url: "/deletewhishlist",
                    data: {
                        productId: proid
                    },
                    method: 'post',
                    success: (response) => {
                        console.log("waoring")

